<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Rapport" >

    <LinearLayout
        android:id="@+id/hej"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <ListView
            android:id="@+id/listRapport"
            android:layout_width="match_parent"
            android:layout_height="392dp"
            android:scrollbars="vertical"
            android:scrollingCache="true" >
        </ListView>
    </LinearLayout>

    <TextView
        android:id="@+id/titleSlider"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/hej"
        android:layout_centerHorizontal="true"
        android:text="@string/rapport_prioritet" />
		<LinearLayout
			android:id="@+id/hej2"
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
		>
		</LinearLayout>
	<RadioGroup
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:layout_below="@+id/titleSlider" android:orientation="horizontal" android:weightSum="100">
		<RadioButton
			android:layout_weight="85"
	        android:id="@+id/rbLow"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:checked="false"
	        android:text="LÃ¥g" />
		<RadioButton
		    android:id="@+id/rbEmergency"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:checked="false"
		    android:text="Akut" />

	</RadioGroup>
</RelativeLayout>